#include <iostream> //input and output stream 

using namespace std;

// Swap the values of two integers
void swap(int &a, int &b) {
    int temp = a;
    a = b;
    b = temp;
}
// Set the initialize num1 with value 2 and num2 with value 1
int main(){
    int num1=2; //num1 with the value 2
    int num2=1;//num2 with the value 1
    //Print the values of num1 and num2 before the swap
    cout << "Before swap: " << endl;
    cout << "num1 = "<<num1 << ", num2 = " << num2 << endl;
    //swap the values of num1 and num2
    swap(num1, num2);
 
    //Print the values of num1 and num2 after the swap
    cout << "After swap:" <<endl;
    cout << "num1 =" << num1 << ", num2 = "<< num2 << endl;
    
    return 0; //program end
}
